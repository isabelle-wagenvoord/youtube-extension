{
	"name": "YouTube Smart Choice",
	"description": "",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["activeTab", "declarativeContent"],
	"content_scripts": [
		{
			"matches": ["https://youtube.com/*"],
			"css": ["background.css"],
			"js": ["background.js"]
		}
	],
	"action": {
		"default_title": "YouTube Smart Choice",
		"default_popup": "./popup/flagged-videos.html",
		"default_icon": {
   		}
	}

}
