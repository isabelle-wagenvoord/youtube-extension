{
	"name": "YouTube Hero",
	"description": "Doot",
	"version": "1.0",
	"manifest_version": "3",
	"permissions": ["activeTab", "declarativeContent"],
	"content_scripts": [
		{
			"matches": ["youtube.com/*"],
			"css": ["background.css"],
			"js": ["background.js"]
		}
	],
	"browser_action": {
		"default_title": "YouTube Extension Popup",
		"default_popup": "popup.html",
		"default_icon": {
   		}
	}

}
